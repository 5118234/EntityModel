    <!--我要合作,邮件接收者邮箱地址-->
    <add key="EmailTo" value="wuhuaizhi@yizuanbao.cn" />

    <!--我要合作,邮件被抄送人地址-->
    <add key="EmailCC" value="xiangchunyan@yizuanbao.cn" />

    <!--我要合作,发送邮件账号-->
