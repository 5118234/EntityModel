    <!--我要合作,发送邮件密码-->
    <add key="EmailUserPassword" value="YiZuanBao123" />

    <!--日志地址-->
    <add key="LogPath" value="c:\log\GoodLin.User.External.Service.Api" />
    <!--日志记录的地址-->
    <add key="LogPath" value="c:\log\auth" />
    <!--是否开启跟踪日志-->
    <add key="LogMonitor" value="False" />
    <!--是否开启SQL语句日志-->
    <add key="LogSql" value="False" />
    <!--以分钟为单位的RefreshToken过期时间，-1表示不过期-->
    <add key="RefreshTokenExpiresMinute" value="-1" />
    <!--以分钟为单位的AccessToken过期时间，-1表示不过期-->
    <add key="AccessTokenExpiresMinute" value="-1" />
    <!--服务器唯一标识-->
    <add key="ServiceKey" value="77E6A88F-390E-49FC-BD5F-B9E43C45ED5C" />
    <!--服务器唯一标识-->
    <add key="OAuthServiceURL" value="http://localhost:19666" />
    <add key="UCURL" value="http://localhost:16542" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
